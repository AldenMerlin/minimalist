    "document-register-element": "^1.7.2",
    "eslint": "^4.19.1",
    "eslint-config-mn-component": "^1.1.0",
    "eslint-plugin-jest": "^21.18.0",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "pug-html-loader": "^1.1.5",
    "sass-loader": "^7.1.0",
    "sass-webpack-plugin": "^1.0.3",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.1.0"
