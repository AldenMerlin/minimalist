{
  "name": "minimalist-ui",
  "private": true,
  "version": "1.0.0",
  "description": "A UI Kit for Web, based in web components (using Custom Elements), ready to be used with or without any framework.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "storybook": "start-storybook -p 3000",
    "prepublish": "babel src --out-dir dist",
    "build-storybook": "build-storybook",
    "start": "npm run pug -- --watch hotsite/index.pug & npm run vendor-sass & npm run app-sass -- --watch & npm run javascript & npm run local-server",
    "local-server": "browser-sync start --server 'docs' --files 'docs/*.*' --no-ui --no-notify",
    "pretest": "npm run lint",
    "lint": "eslint *.js src/**/*.js --quiet",
    "vendor-sass": "cat src/**/*.scss | node-sass --output-style compressed > docs/vendor.css",
    "app-sass": "node-sass hotsite/app.scss docs/app.css -q --output-style compressed --source-map true --error-bell",
    "pug": "pug hotsite/index.pug --out docs/ --silent",
    "javascript": "webpack-cli --config webpack.config.js"
  },
  "keywords": [],
  "author": "Darlan Mendon√ßa",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@storybook/addon-knobs": "4.0.0-alpha.6",
    "@storybook/addons": "4.0.0-alpha.6",
    "@storybook/react": "4.0.0-alpha.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-import-export": "^1.0.2",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.24.6",
    "chai": "^4.1.2",
    "chai-dom": "^1.7.0",
    "chai-enzyme": "^1.0.0-beta.0",
    "chai-spies": "^1.0.0",
    "css-loader": "^0.28.11",
    "document-register-element": "^1.7.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-mn-component": "^1.1.0",
    "eslint-plugin-jest": "^21.18.0",
    "jest": "^22.4.3",
    "jest-css-modules": "^1.1.0",
    "jsdom": "^11.6.2",
    "markdown-loader": "^3.0.0",
    "mutation-observer": "^1.0.3",
    "node-sass": "^4.8.3",
    "open-color": "^1.6.3",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "sass-loader": "^6.0.7",
    "storybook-addon-jsx": "^5.3.0",
    "storybook-readme": "^3.3.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@webcomponents/custom-elements": "^1.1.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
