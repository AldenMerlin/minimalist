{
  "name": "minimalist-ui",
  "private": true,
  "version": "1.0.0",
  "description": "a complete and responsive interface for web",
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run build:hotsite -- --watch",
    "build": "npm run build:hotsite && npm run build:module",
    "build:hotsite": "webpack-cli",
    "build:module": "babel src/index.js src/**/*.class.js --out-dir ./dist",
    "test": "jest --coverage",
    "prepublish": "npm run build:module",
    "pretest": "npm run lint",
    "lint": "eslint *.js src/**/*.js --quiet",
    "storybook": "start-storybook -p 4000 -c .storybook"
  },
  "keywords": [],
  "author": "Darlan Mendon√ßa <darlanmendonca@gmail.com> (https://darlanmendonca.github.io)",
  "dependencies": {
    "open-color": "^1.6.3",
    "@webcomponents/custom-elements": "^1.1.3"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/html": "^4.1.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-import-export": "^1.0.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "chai": "^4.1.2",
    "chai-dom": "^1.8.1",
